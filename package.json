{
  "author": "Tom de Grunt <tom@degrunt.nl>",
  "name": "jsonschema",
  "version": "1.0.2",
  "license": "MIT",
  "dependencies": {},
  "contributors": [
    {
      "name": "Austin Wright"
    }
  ],
  "main": "./dist/lib",
  "devDependencies": {
    "babel-cli": "~6.0.12",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.0.12",
    "chai": "~1.5.0",
    "mocha": "~1.8.2"
  },
  "optionalDependencies": {},
  "engines": {
    "node": "*"
  },
  "keywords": [
    "json",
    "schema",
    "jsonschema",
    "validator",
    "validation"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/tdegrunt/jsonschema.git"
  },
  "description": "A fast and easy to use JSON Schema validator",
  "scripts": {
    "test": "./node_modules/.bin/mocha -R spec",
    "compile": "./node_modules/.bin/babel --optional runtime lib --out-dir dist",
    "prepublish": "npm run compile",
    "pretest": "npm run compile"
  },
  "babel": {
    "plugins": ["transform-es2015-modules-commonjs"]
  }
}
